{
    "name": "mintware-de/native-cron-bundle",
    "description": "A Symfony bundle for mintware-de/native-cron",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.1",
        "mintware-de/native-cron": "^1.1.0",
        "symfony/http-kernel": "^6.1",
        "symfony/dependency-injection": "^6.1",
        "symfony/config": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.8",
        "friendsofphp/php-cs-fixer": "^3.13"
    },
    "scripts": {
        "fix-cs": "php-cs-fixer fix",
        "fix-cs:ci": "php-cs-fixer fix --stop-on-violation --dry-run",
        "analyse": "phpstan analyse",
        "test": "phpunit"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MintwareDe\\NativeCronBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MintwareDe\\NativeCronBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Julian Finkler",
            "email": "julian@mintware.de"
        }
    ]
}
